                echo 'Hello World'
		echo 'testing'
            }
        }
        stage('Build') {
            steps {
                echo 'Building'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing '
            }
        }
        stage('Release') {
            steps {
                echo 'Releasing'
            }
        }
    }
}
